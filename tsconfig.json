{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "CommonJS",
    "target": "ES2019",
    "lib": [
      "ES2020"
    ],
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "typeRoots": [
      "node_modules/@types",
      "types"
    ]
  },
  "include": [
    "function"
  ],
  "exclude": [
    "node_modules"
  ]
}
